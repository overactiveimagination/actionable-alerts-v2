			/* ========== Footer Styles ========== */

			#templateFooter{ 
				background-color:#ffffff;
			}

			.footerContent{
				padding-top:20px;
				padding-right:0px;
				padding-bottom:0px;
				padding-left:0px;
				text-align:left;
